<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bianwenkai.mapper.DustEnvDataMapper">

    <select id="GetEnvData" resultType="DustEnvironment">
        select *
        from environment_info
        where id between #{start} and #{end};
    </select>

    <select id="GetWarningData" resultType="WarningData">
        select dust_limit,temperature_limit,humidity_limit,wind_speed_limit
        from warning_data
    </select>

</mapper>