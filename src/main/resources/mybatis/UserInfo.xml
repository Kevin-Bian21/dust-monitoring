<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bianwenkai.mapper.UserMapper">

    <select id="AllUserInfo" resultType="User">
        select user_id,user_name,login_account,user_pwd as password, avatar , phone,email,generate_time,last_login_time,login_count
        from user_info;
    </select>

    <delete id="DeleteUser">
        delete
        from user_info
        where user_id = #{userId}
    </delete>

</mapper>